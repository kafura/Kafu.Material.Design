{
  "name": "Kafu.Material.Design",
  "description": "A material design ",
  "version": "0.0.1",
  "keywords": [
    "html",
    "css",
    "js",
    "javascript",
    "less",
    "mobile-first",
    "responsive",
    "rwd",
    "front-end",
    "framework",
    "boilerplate",
    "web"
  ],
  "scripts": {
    "uglify:css": "node_modules/less/bin/lessc src/less/kmd.less | node_modules/uglifycss/uglifycss > build/kmd.min.css",
    "build:css": "node_modules/less/bin/lessc src/less/kmd.less build/kmd.css",
    "watch-css": "watch-run -p 'src/less/*.less' 'npm run build:css && npm run uglify:css'"
  },
  "homepage": "http://www.kafu.dk",
  "author": "Ralf Bach",
  "repository": {
    "type": "git",
    "url": "https://github.com/kafura/Kafu.Material.Design.git"
  },
  "bugs": {
    "url": "https://github.com/kafura/Kafu.Material.Design/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/kafura/Kafu.Material.Design/issues"
    }
  ],
  "devDependencies": {
    "less": "^2.6.0",
    "uglifycss": "0.0.20",
    "watch-run": "^1.2.4"
  }
}
